#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#include<windows.h>
void print(int n)
{
	int i, j;
	for (i = 1; i <= n; i++)
	{
		for (j = 1; j <= i; j++)
		{
			printf("%d*%d=%-3d ", i, i, i * j);
		}
		printf("\n");
	}
}

int main()
{
	int n;
	scanf("%d",&n);
	print(n);
    return 0;
}
